
@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>
<form method="post" enctype="multipart/form-data">
    <div class="form-group">Staff Name</div>
    <div><input type="text" name="staffname" class="form-control" /></div>
    <div class="form-group">Staff Email</div>
    <div><input type="text" name="staffemail" class="form-control" /></div>
   
       

    <div>@ViewBag.ErrorMessage</div>

    <div><button type="submit" class="btn-success">Submit Here</button></div>

</form>

